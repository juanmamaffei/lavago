<%= react_component("Profile", @user) %>

<p>
    <%= if user_signed_in? then link_to('Cerrar sesión', destroy_user_session_path, method: :delete) else link_to('Iniciar sesión', new_user_session_path) end %>
</p>
<% if (@incomplete_profile == true) %>
  <%= react_component("Profile", @user) %>
<% else %>
  <ul>
      <li><%= link_to('Productos',products_path) %></li>
      <li><%= link_to('Lavanderías',laundries_path) %></li>
      <li><%= link_to('Suscripciones',suscriptions_path) %></li>
      <li><%= link_to('Pedidos',orders_path) %></li>
      <li><%= link_to('Cadetes',carriers_path) %></li>

  </ul>
<% end %>
Datos personales
<ul>
  <li>Email: <%= @user.email %></li>
  <li>Nombre: <%= @user.name %></li>
  <li>Apellido: <%= @user.last_name %></li>
  <li>Localidad: <%= @user.city %></li>
  <li>Dirección: <%= @user.address %></li>
  <li>Detalles de dirección: <%= @user.address_details %></li>
  <li>Puntaje por tus pedidos: <%= @user.score %></li>
 
</ul>
